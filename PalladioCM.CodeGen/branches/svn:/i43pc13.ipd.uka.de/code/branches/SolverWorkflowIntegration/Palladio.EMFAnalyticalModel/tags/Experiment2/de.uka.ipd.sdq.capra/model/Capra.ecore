<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="capra"
    nsURI="http://sdq.ipd.uka.de/Capra/1.0" nsPrefix="capra">
  <eClassifiers xsi:type="ecore:EClass" name="ReplicatedProcess">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="numReplicas" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="processvariable" lowerBound="1"
        eType="#//core/ProcessVariable" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CapraModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="processes" upperBound="-1"
        eType="#//ReplicatedProcess" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="resources" upperBound="-1"
        eType="#//resources/Resource" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="identifiers" upperBound="-1"
        eType="#//core/Identifier" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sensors" upperBound="-1"
        eType="#//measurement/Sensor" containment="true"/>
  </eClassifiers>
  <eSubpackages name="core" nsURI="http://sdq.ipd.uka.de/Capra/Core/1.0" nsPrefix="capra.core">
    <eClassifiers xsi:type="ecore:EClass" name="CapraExpression" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="Terminal" abstract="true" eSuperTypes="#//core/CapraExpression"/>
    <eClassifiers xsi:type="ecore:EClass" name="Operator" abstract="true" eSuperTypes="#//core/CapraExpression"/>
    <eClassifiers xsi:type="ecore:EClass" name="Action" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="EventAction" abstract="true" eSuperTypes="#//core/Action">
      <eStructuralFeatures xsi:type="ecore:EReference" name="identifier" lowerBound="1"
          eType="#//core/CommunicationIdentifier"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CommunicationIdentifier" eSuperTypes="#//core/Identifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Identifier" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral=""/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DemandAction" eSuperTypes="#//core/Action">
      <eStructuralFeatures xsi:type="ecore:EReference" name="identifier" lowerBound="1"
          eType="#//core/DemandIdentifier"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="resourceUsage" lowerBound="1"
          eType="#//core/ResourceUsage" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DemandIdentifier" eSuperTypes="#//core/Identifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="ResourceUsage">
      <eStructuralFeatures xsi:type="ecore:EReference" name="usageTime" lowerBound="1"
          eType="ecore:EClass ../../de.uka.ipd.sdq.probfunction/model/probfunction.ecore#//ProbabilityDensityFunction"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="resource" lowerBound="1"
          eType="#//resources/ProcessingResource"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SuccessfulTermination" eSuperTypes="#//core/Terminal"/>
    <eClassifiers xsi:type="ecore:EClass" name="ProbabilisticPrefix" eSuperTypes="#//core/Operator">
      <eStructuralFeatures xsi:type="ecore:EReference" name="action" unique="false"
          lowerBound="1" eType="#//core/Action" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="targetOptions" upperBound="-1"
          eType="#//core/InternalSelector" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="InternalSelector">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="probability" unique="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="1"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="process" lowerBound="1"
          eType="#//core/CapraExpression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Choice" eSuperTypes="#//core/Operator">
      <eStructuralFeatures xsi:type="ecore:EReference" name="options" unique="false"
          upperBound="-1" eType="#//core/ExternalSelector" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ExternalSelector">
      <eStructuralFeatures xsi:type="ecore:EReference" name="input" lowerBound="1"
          eType="#//core/InputEventAction" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="targetProcess" lowerBound="1"
          eType="#//core/CapraExpression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="InputEventAction" eSuperTypes="#//core/EventAction"/>
    <eClassifiers xsi:type="ecore:EClass" name="OutputEventAction" eSuperTypes="#//core/EventAction"/>
    <eClassifiers xsi:type="ecore:EClass" name="CommunicationEventAction" eSuperTypes="#//core/EventAction"/>
    <eClassifiers xsi:type="ecore:EClass" name="BoundedLoop" eSuperTypes="#//core/Operator">
      <eStructuralFeatures xsi:type="ecore:EReference" name="repeatedProcess" lowerBound="1"
          eType="#//core/CapraExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="targetProcess" lowerBound="1"
          eType="#//core/CapraExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="numberOfIterations" lowerBound="1"
          eType="ecore:EClass ../../de.uka.ipd.sdq.probfunction/model/probfunction.ecore#//ProbabilityMassFunction"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ParallelComposition" eSuperTypes="#//core/Operator">
      <eStructuralFeatures xsi:type="ecore:EReference" name="processes" upperBound="-1"
          eType="#//core/CapraExpression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Restriction" eSuperTypes="#//core/Operator">
      <eStructuralFeatures xsi:type="ecore:EReference" name="restrictedProcess" lowerBound="1"
          eType="#//core/CapraExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="restrictedIdentifiers"
          upperBound="-1" eType="#//core/CommunicationIdentifier"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Renaming" eSuperTypes="#//core/Operator">
      <eStructuralFeatures xsi:type="ecore:EReference" name="renamedProcess" lowerBound="1"
          eType="#//core/CapraExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mapping" upperBound="-1"
          eType="#//core/Mapping" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Mapping">
      <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
          eType="#//core/Identifier"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
          eType="#//core/Identifier"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SilentIdentifier" eSuperTypes="#//core/DemandIdentifier #//core/CommunicationIdentifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="ProcessVariable">
      <eStructuralFeatures xsi:type="ecore:EReference" name="process" lowerBound="1"
          eType="#//core/CapraExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral=""/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ProcessVariableUsage" eSuperTypes="#//core/CapraExpression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="processVariable" lowerBound="1"
          eType="#//core/ProcessVariable"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="resources" nsURI="http://sdq.ipd.uka.de/Capra/Resources/1.0"
      nsPrefix="capra.resources">
    <eClassifiers xsi:type="ecore:EClass" name="Resource" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral=""/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ProcessingResource" eSuperTypes="#//resources/ContentionResource #//resources/ActiveResource">
      <eStructuralFeatures xsi:type="ecore:EReference" name="scheduler" lowerBound="1"
          eType="#//schedulers/Scheduler" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ContentionResource" abstract="true"
        eSuperTypes="#//resources/Resource">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="capacity" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt" defaultValueLiteral="1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ActiveResource" abstract="true" eSuperTypes="#//resources/Resource"/>
    <eClassifiers xsi:type="ecore:EClass" name="DelayResource" eSuperTypes="#//resources/ActiveResource"/>
  </eSubpackages>
  <eSubpackages name="schedulers" nsURI="http://sdq.ipd.uka.de/Capra/Schedulers/1.0"
      nsPrefix="capra.schedulers">
    <eClassifiers xsi:type="ecore:EClass" name="Scheduler">
      <eStructuralFeatures xsi:type="ecore:EReference" name="assignment" lowerBound="1"
          eType="#//schedulers/Assignment" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="selectionpolicy" lowerBound="1"
          eType="#//schedulers/SelectionPolicy" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="timeconsumption" lowerBound="1"
          eType="#//schedulers/TimeConsumption" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral=""/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="loadBalancing" lowerBound="1"
          eType="#//schedulers/LoadBalancing"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Assignment" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="SelectionPolicy" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="TimeConsumption">
      <eStructuralFeatures xsi:type="ecore:EReference" name="time" lowerBound="1"
          eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="scaling" lowerBound="1"
          eType="#//schedulers/BigOClass"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="BigOClass">
      <eLiterals name="Constant"/>
      <eLiterals name="Linear" value="1"/>
      <eLiterals name="Square" value="2"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="LoadBalancing">
      <eLiterals name="OnIdle"/>
      <eLiterals name="OnThreshold" value="1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="NonPreemptive" eSuperTypes="#//schedulers/Assignment"/>
    <eClassifiers xsi:type="ecore:EClass" name="Preemptive" eSuperTypes="#//schedulers/Assignment">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="immediate" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="quantum" lowerBound="1"
          eType="#//schedulers/Quantum" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Quantum">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="timeSlice" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="0"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="WaitingTime" eSuperTypes="#//schedulers/SelectionPolicy"/>
    <eClassifiers xsi:type="ecore:EClass" name="Priority" eSuperTypes="#//schedulers/SelectionPolicy">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="hasDynamicPriority" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="preference" lowerBound="1"
          eType="#//schedulers/Preference"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="Preference">
      <eLiterals name="Hard"/>
      <eLiterals name="Soft" value="1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="PriorityType">
      <eLiterals name="Static"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PriortyQuantums">
      <eStructuralFeatures xsi:type="ecore:EReference" name="priority" lowerBound="1"
          eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="numberOfQuantums" lowerBound="1"
          eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="measurement" nsURI="http://sdq.ipd.uka.de/Capra/Measurements/1.0"
      nsPrefix="capra.measurement">
    <eClassifiers xsi:type="ecore:EClass" name="TimeProbe" eSuperTypes="#//measurement/ProbeAction">
      <eStructuralFeatures xsi:type="ecore:EReference" name="sensorsToStart" upperBound="-1"
          eType="#//measurement/TimeSpanSensor" eOpposite="#//measurement/TimeSpanSensor/startMeasurement"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="sensorsToStop" upperBound="-1"
          eType="#//measurement/TimeSpanSensor" eOpposite="#//measurement/TimeSpanSensor/stopMeasurement"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ProbeAction" abstract="true" eSuperTypes="#//core/Action"/>
    <eClassifiers xsi:type="ecore:EClass" name="TimeSpanSensor" eSuperTypes="#//measurement/Sensor">
      <eStructuralFeatures xsi:type="ecore:EReference" name="timeSpanRecorders" upperBound="-1"
          eType="#//measurement/TimeSpanRecorder" containment="true" eOpposite="#//measurement/TimeSpanRecorder/timeSpanSensor"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="startMeasurement" lowerBound="1"
          eType="#//measurement/TimeProbe" eOpposite="#//measurement/TimeProbe/sensorsToStart"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="stopMeasurement" lowerBound="1"
          eType="#//measurement/TimeProbe" eOpposite="#//measurement/TimeProbe/sensorsToStop"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Sensor" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral=""/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TimeSpanRecorder" abstract="true"
        eSuperTypes="#//measurement/Recorder">
      <eStructuralFeatures xsi:type="ecore:EReference" name="timeSpanSensor" lowerBound="1"
          eType="#//measurement/TimeSpanSensor" eOpposite="#//measurement/TimeSpanSensor/timeSpanRecorders"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Recorder" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EReference" name="startCondition" lowerBound="1"
          eType="#//measurement/Condition" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="stopCondition" lowerBound="1"
          eType="#//measurement/Condition" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Condition"/>
    <eClassifiers xsi:type="ecore:EClass" name="VisitProbe" eSuperTypes="#//measurement/ProbeAction"/>
    <eClassifiers xsi:type="ecore:EClass" name="StateSensor" eSuperTypes="#//measurement/Sensor">
      <eStructuralFeatures xsi:type="ecore:EReference" name="stateRecorders" upperBound="-1"
          eType="#//measurement/StateRecorder" containment="true" eOpposite="#//measurement/StateRecorder/stateSensor"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="StateRecorder" abstract="true" eSuperTypes="#//measurement/Recorder">
      <eStructuralFeatures xsi:type="ecore:EReference" name="stateSensor" lowerBound="1"
          eType="#//measurement/StateSensor" eOpposite="#//measurement/StateSensor/stateRecorders"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CountingSensor" eSuperTypes="#//measurement/Sensor">
      <eStructuralFeatures xsi:type="ecore:EReference" name="countingRecorders" upperBound="-1"
          eType="#//measurement/CountingRecorder" containment="true" eOpposite="#//measurement/CountingRecorder/countingSensor"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="countVisits" lowerBound="1"
          eType="#//measurement/VisitProbe"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CountingRecorder" abstract="true"
        eSuperTypes="#//measurement/Recorder">
      <eStructuralFeatures xsi:type="ecore:EReference" name="countingSensor" lowerBound="1"
          eType="#//measurement/CountingSensor" eOpposite="#//measurement/CountingSensor/countingRecorders"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="HistogramRecorder" eSuperTypes="#//measurement/TimeSpanRecorder">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="stepWidth" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
          defaultValueLiteral="1.0"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="FullTimeSpanRecorder" eSuperTypes="#//measurement/TimeSpanRecorder"/>
    <eClassifiers xsi:type="ecore:EClass" name="SteadyStateRecorder" eSuperTypes="#//measurement/StateRecorder"/>
    <eClassifiers xsi:type="ecore:EClass" name="FullStateRecorder" eSuperTypes="#//measurement/StateRecorder"/>
    <eClassifiers xsi:type="ecore:EClass" name="FinalValueRecorder" eSuperTypes="#//measurement/CountingRecorder"/>
  </eSubpackages>
  <eSubpackages name="extension" nsURI="http://sdq.ipd.uka.de/Capra/Extensions/1.0"
      nsPrefix="capra.extension">
    <eClassifiers xsi:type="ecore:EClass" name="PassiveResource" abstract="true" eSuperTypes="#//resources/ContentionResource"/>
    <eClassifiers xsi:type="ecore:EClass" name="AcquireAction" eSuperTypes="#//core/EventAction">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="number" unique="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="resource" lowerBound="1"
          eType="#//extension/PassiveResource"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ReleaseAction" eSuperTypes="#//core/EventAction">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="number" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="resource" lowerBound="1"
          eType="#//extension/PassiveResource"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="WeakSemaphore" eSuperTypes="#//extension/PassiveResource"/>
    <eClassifiers xsi:type="ecore:EClass" name="StrongSemaphore" eSuperTypes="#//extension/PassiveResource"/>
    <eClassifiers xsi:type="ecore:EClass" name="SchedulingStrategy"/>
  </eSubpackages>
</ecore:EPackage>
