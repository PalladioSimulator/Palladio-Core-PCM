        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="self.failureOccurrenceDescriptions->forAll(x : FailureOccurrenceDescription,y:FailureOccurrenceDescription  | x&lt;>y implies x.failureType &lt;> y.failureType )"/>
        </eAnnotations>
        <eParameters name="diagnostics" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDiagnosticChain">
