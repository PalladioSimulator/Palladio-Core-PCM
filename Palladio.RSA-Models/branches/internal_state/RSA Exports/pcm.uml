        <ownedAttribute xmi:id="_kAS_EPwtEd2ByapuHVVWnA" name="passiveResource_BasicComponent" type="_kAS-XvwtEd2ByapuHVVWnA" aggregation="composite" association="_kAS_fPwtEd2ByapuHVVWnA">
