          <lowerValue xmi:type="uml:LiteralInteger" xmi:id="_kAS-x_wtEd2ByapuHVVWnA"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="_kAS-yPwtEd2ByapuHVVWnA" name="ancestorInterfaces_Interface" type="_kAS-vfwtEd2ByapuHVVWnA" isDerived="true" association="_kAS_KPwtEd2ByapuHVVWnA">
