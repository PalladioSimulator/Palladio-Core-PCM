        </ownedAttribute>
        <ownedAttribute xmi:id="_kAS_FPwtEd2ByapuHVVWnA" name="state_BasicComponent" visibility="private" type="_kAS_jvwtEd2ByapuHVVWnA" aggregation="composite" association="_kAS_kPwtEd2ByapuHVVWnA">
