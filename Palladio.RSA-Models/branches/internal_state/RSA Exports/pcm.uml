        </ownedAttribute>
        <ownedAttribute xmi:id="_kAS-zPwtEd2ByapuHVVWnA" name="protocols__Interface" type="_kAS_n_wtEd2ByapuHVVWnA" aggregation="composite" association="_kAS_JvwtEd2ByapuHVVWnA">
